    "url": "https://github.com/locaulker/mern-commerce/issues"
  },
  "homepage": "https://github.com/locaulker/mern-commerce#readme",
  "dependencies": {
    "bcryptjs": "2.4.3",
    "colors": "1.4.0",
    "dotenv": "8.2.0",
    "express": "4.17.1",
    "express-async-handler": "^1.1.4",
    "jsonwebtoken": "8.5.1",
    "mongoose": "5.10.7"
  },
  "devDependencies": {
    "concurrently": "5.3.0",
    "nodemon": "2.0.4"
